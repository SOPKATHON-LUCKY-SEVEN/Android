<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Soptkathon" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:includeFontPadding">false</item>
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="textAppearanceHeadline1">@style/TextAppearance.Star23.HeadLine1</item>
        <item name="textAppearanceHeadline2">@style/TextAppearance.Star23.HeadLine2</item>
        <item name="textAppearanceHeadline3">@style/TextAppearance.Star23.HeadLine3</item>
        <item name="textAppearanceHeadline4">@style/TextAppearance.Star23.HeadLine4</item>
        <item name="textAppearanceHeadline5">@style/TextAppearance.Star23.HeadLine5</item>
        <item name="textAppearanceSubtitle1">@style/TextAppearance.Star23.SubTitle1</item>
        <item name="textAppearanceSubtitle2">@style/TextAppearance.Star23.SubTitle2</item>
        <item name="textAppearanceBody1">@style/TextAppearance.Star23.Body1</item>
    </style>

    <style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined" parent="">
        <item name="colorControlActivated">@color/black</item>
        <item name="colorPrimary">@color/black</item>
    </style>
</resources>